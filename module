#include <stdio.h>

int main() {
  int a, b;

  a = 12;
  b = 20;
  printf("a = %d, b = %d \n", a, b);
  {
    int a, c;

    a = 30;
    c = 50;
    printf("a = %d, b = %d, c = %d \n", a, b, c);

    a = 90;
    b = 45;
    c = 75;
  }
  printf("a = %d, b = %d \n", a, b);  
  return 0;
}


#include <stdio.h>
int ga;
int gb;
void Sum();
void Difference();
int main() {
  printf("정수를 두개 입력하세요 : ");
  scanf("%d %d", &ga, &gb);
  Sum();
  Difference();
  return 0;
}
void Sum(){
  printf("%d + %d = %d\n", ga, gb, ga + gb);
}
void Difference()
{
  int diff;
  if (ga > gb)
  {
    diff = ga - gb;
    printf("%d - %d = ", ga, gb);
  }
  else{
    diff = gb - ga;
    printf("%d - %d = ", gb, ga);
  }
  printf("%d", diff);
}

#include <stdio.h>
void Ccube();
void Coding();
int main() {
  Ccube();
  Ccube();
  Ccube();
  Coding();
  Coding();
  Coding();
  return 0;
}
void Ccube(){
  int cnt = 0;

  cnt++;
  printf("%d 씨큐브\n", cnt);
}
void Coding()
{
  static int cnt = 0;
  cnt++;
  printf("%d 코딩 \n", cnt);
}

#include <stdio.h>
#define Max 10
#define PI 3.141592
#define C3C "씨큐브코딩"
#define PRN printf
#define MACRO "## 매크로 상수##\n"
#define SUM(n, m) ((n)+(m))
#define SUb(s, b) ((s)-(b))
int main() {
  PRN(MACRO);
  PRN("Max = %d\n", Max);
  PRN("PI = %lf\n", PI);
  PRN("C3C = %s\n", C3C);
  PRN("%d\n", SUM(3, 8));
  PRN("%lf\n", SUM(3.6, 8.34));
  PRN("%d\n", SUb(13, 5));
  PRN("%lf\n", SUb(4.2, 3.4));
  return 0;
}
